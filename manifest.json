{
    "manifest_version": 3,
    "name": "Quick Event Adder",
    "version": "1.0",
    "description": "Add calendar events quickly with natural language.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    
    },
    "permissions": ["tabs"],
    "host_permissions": ["https://calendar.google.com/*"]
  }
  